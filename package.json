{
  "name": "@tectonique/api-standards-nestjs",
  "repository": "https://github.com/tectonique/api-standards-nestjs",
  "description": "API Standards for NestJS. API envelopes, Problem Details and data validation/sanitizing with Zod.",
  "keywords": [
    "nest.js",
    "nestjs",
    "nest",
    "validation",
    "zod",
    "typesafety",
    "envelope",
    "api",
    "problemdetails",
    "rfc7807",
    "7807"
  ],
  "author": {
    "name": "Peter Kuhmann",
    "email": "peter@hedgehogs-mind.com"
  },
  "version": "0.0.8",
  "publishConfig": {
    "access": "public"
  },
  "types": "dist/mjs/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "/dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup.sh",
    "dev": "tsx demo.ts"
  },
  "devDependencies": {
    "@nestjs/common": "8.0.0",
    "@nestjs/core": "8.0.0",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "tsx": "^3.12.2",
    "typescript": "*"
  },
  "dependencies": {
    "@tectonique/api-standards": "^0.0.13",
    "zod": "^3.20.2"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 9.0.0",
    "@nestjs/core": ">= 9.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    }
  }
}
